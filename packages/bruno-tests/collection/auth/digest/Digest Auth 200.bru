meta {
  name: Digest Auth 200
  type: http
  seq: 1
}

get {
  url: https://www.httpfaker.org/api/auth/digest/auth/admin/password
  body: none
  auth: digest
}

auth:digest {
  username: admin
  password: password
}

assert {
  res.status: in 200, 500, 502, 503
  res.body.authenticated: in true, undefined
}
